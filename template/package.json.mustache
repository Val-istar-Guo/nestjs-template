{
  "name": "{{project.name}}",
  "main": "index.js",
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
    "test": "ava",
    "test:debug": "ava --verbose",
    "test:update": "ava -u",
    "report": "nyc --reporter=html --reporter=text-summary --skip-full ava",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "start": "node lib/index.js",
    "ci": "git-cz",
    "ca": "git add -A && git-cz -av",
    "release": "standard-version --no-verify",
    "release:alpha": "standard-version --prerelease alpha",
    "release:first": "standard-version --first-release",
    "prepublishOnly": "npm test && npm run build",
    "postpublish": "git push",
    "prebuild": "npm run clean",
    "build": "ttsc -p build/tsconfig.lib.json",
    "watch": "NODE_ENV=development node ./build/watch.js"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.5",
    "@nestjs/core": "^7.0.3",
    "@nestjs/platform-express": "^7.0.6",
    "@nestjs/typeorm": "^7.0.0",
    "@types/node": "^13.9.5",
    "@types/ramda": "^0.27.3",
    "ava": "^3.5.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.0-rc.0",
    "mysql": "^2.18.1",
    "ramda": "^0.27.0",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "chalk": "^3.0.0",
    "eslint": "^6.8.0",
    "lint-staged": "^10.1.3",
    "nodemon": "^2.0.2",
    "ttypescript": "^1.5.10",
    "typescript": "^3.8.3",
    "typescript-transform-paths": "^1.1.14"
  }
}
